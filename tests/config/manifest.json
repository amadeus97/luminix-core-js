{
    "models": {
        "file": {
            "fillable": [
                "path",
                "type",
                "attachment_id"
            ],
            "softDeletes": true,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "attachments": {
                    "type": "BelongsTo",
                    "model": "attachment"
                }
            },
            "casts": {
                "id": "int"
            },
            "class": "App\\Models\\File"
        },
        "attachment": {
            "fillable": [
                "path",
                "type",
                "attachable_id",
                "attachable_type",
                "size"
            ],
            "softDeletes": true,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "attachable": {
                    "type": "MorphTo",
                    "model": "attachment"
                },
                "author": {
                    "type": "BelongsTo",
                    "model": "user"
                },
                "post": {
                    "type": "BelongsTo",
                    "model": "post"
                },
                "file": {
                    "type": "HasOne",
                    "model": "file"
                }
            },
            "casts": {
                "id": "int",
                "size": "integer"
            },
            "class": "App\\Models\\Attachment"
        },
        "post_comment": {
            "fillable": [
                "post_id",
                "user_id",
                "body"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "post": {
                    "type": "BelongsTo",
                    "model": "post"
                },
                "user": {
                    "type": "BelongsTo",
                    "model": "user"
                }
            },
            "casts": {
                "id": "int"
            },
            "class": "App\\Models\\PostComment"
        },
        "post": {
            "fillable": [
                "title",
                "description",
                "author_id",
                "content",
                "slug",
                "published_at",
                "published"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "author": {
                    "type": "BelongsTo",
                    "model": "user"
                },
                "comments": {
                    "type": "HasMany",
                    "model": "post_comment"
                },
                "attachments": {
                    "type": "MorphMany",
                    "model": "attachment"
                }
            },
            "casts": {
                "id": "int",
                "content": "Arandu\\LaravelSafeJsonCast\\Json",
                "published_at": "datetime",
                "published": "boolean",
                "likes": "integer"
            },
            "class": "App\\Models\\Post"
        },
        "user": {
            "fillable": [
                "name",
                "email",
                "password"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "posts": {
                    "type": "HasMany",
                    "model": "post"
                },
                "comments": {
                    "type": "HasMany",
                    "model": "post_comment"
                },
                "attachments": {
                    "type": "HasMany",
                    "model": "attachment"
                },
                "chairs": {
                    "type": "BelongsToMany",
                    "model": "chair"
                }
            },
            "casts": {
                "id": "int",
                "email_verified_at": "datetime",
                "password": "hashed"
            },
            "class": "App\\Models\\User"
        },
        "chair": {
            "fillable": [
                "name", 
                "description"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "users": {
                    "type": "BelongsToMany",
                    "model": "user"
                }
            },
            "casts": {
                "id": "int"
            },
            "class": "App\\Models\\Chair"
        }
    },
    "routes": {
        "luminix": {
            "post_comment": {
                "index": ["api\/luminix\/post_comments", "get"],
                "show": ["api\/luminix\/post_comments\/{id}", "get"],
                "store": ["api\/luminix\/post_comments", "post"],
                "update": ["api\/luminix\/post_comments\/{id}\/update", "put"],
                "destroy": ["api\/luminix\/post_comments\/{id}", "delete"],
                "destroyMany": ["api\/luminix\/post_comments", "delete"]
            },
            "post": {
                "index": ["api\/luminix\/posts", "get"],
                "show": ["api\/luminix\/posts\/{id}", "get"],
                "store": ["api\/luminix\/posts", "post"],
                "update": ["api\/luminix\/posts\/{id}", "put"],
                "destroy": ["api\/luminix\/posts\/{id}", "delete"],
                "destroyMany": ["api\/luminix\/posts", "delete"]
            },
            "user": {
                "index": ["api\/luminix\/users", "get"],
                "show": ["api\/luminix\/users\/{id}", "get"],
                "store": ["api\/luminix\/users", "post"],
                "update": ["api\/luminix\/users\/{id}", "put"],
                "destroy": ["api\/luminix\/users\/{id}", "delete"],
                "destroyMany": ["api\/luminix\/users", "delete"],
                "restoreMany": ["api\/luminix\/users", "put"]
            }
        },
        "login": ["login", "get", "post"],
        "logout": ["logout", "post"],
        "password": {
            "request": ["password\/reset", "get"],
            "email": ["password\/email", "get"],
            "reset": ["password\/reset\/{token}", "get", "post"],
            "update": ["password\/reset", "post"],
            "confirm": ["password\/confirm", "get", "post"]
        },
        "home": ["\/", "get"],
        "profile": ["profile", "get"]
    }
}