{
    "models": {
        "attachment": {
            "fillable": [
                "path",
                "type",
                "attachable_id",
                "attachable_type",
                "size"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "attachable": {
                    "type": "MorphTo",
                    "model": "attachment"
                },
                "author": {
                    "type": "BelongsTo",
                    "model": "user"
                }
            },
            "casts": {
                "id": "int",
                "size": "integer"
            },
            "class": "App\\Models\\Attachment"
        },
        "comment": {
            "fillable": [
                "post_id",
                "user_id",
                "body"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "post": {
                    "type": "BelongsTo",
                    "model": "post"
                },
                "user": {
                    "type": "BelongsTo",
                    "model": "user"
                }
            },
            "casts": {
                "id": "int"
            },
            "class": "App\\Models\\Comment"
        },
        "post": {
            "fillable": [
                "title",
                "description",
                "author_id",
                "content",
                "slug",
                "published_at",
                "published"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "author": {
                    "type": "BelongsTo",
                    "model": "user"
                },
                "comments": {
                    "type": "HasMany",
                    "model": "comment"
                },
                "attachments": {
                    "type": "MorphMany",
                    "model": "attachment"
                }
            },
            "casts": {
                "id": "int",
                "content": "Arandu\\LaravelSafeJsonCast\\Json",
                "published_at": "datetime",
                "published": "boolean",
                "likes": "integer"
            },
            "class": "App\\Models\\Post"
        },
        "user": {
            "fillable": [
                "name",
                "email",
                "password"
            ],
            "softDeletes": false,
            "importable": false,
            "exportable": false,
            "primaryKey": "id",
            "timestamps": true,
            "relations": {
                "posts": {
                    "type": "HasMany",
                    "model": "post"
                },
                "comments": {
                    "type": "HasMany",
                    "model": "comment"
                }
            },
            "casts": {
                "id": "int",
                "email_verified_at": "datetime",
                "password": "hashed"
            },
            "class": "App\\Models\\User"
        }
    },
    "routes": {
        "luminix": {
            "comment": {
                "index": "api\/luminix\/comments",
                "show": "api\/luminix\/comment\/{id}",
                "store": "api\/luminix\/comments",
                "update": "api\/luminix\/comment\/{id}",
                "destroy": "api\/luminix\/comment\/{id}",
                "destroyMany": "api\/luminix\/comments"
            },
            "post": {
                "index": "api\/luminix\/posts",
                "show": "api\/luminix\/post\/{id}",
                "store": "api\/luminix\/posts",
                "update": "api\/luminix\/post\/{id}",
                "destroy": "api\/luminix\/post\/{id}",
                "destroyMany": "api\/luminix\/posts"
            },
            "user": {
                "index": "api\/luminix\/users",
                "show": "api\/luminix\/user\/{id}",
                "store": "api\/luminix\/users",
                "update": "api\/luminix\/user\/{id}",
                "destroy": "api\/luminix\/user\/{id}",
                "destroyMany": "api\/luminix\/users",
                "restoreMany": "api\/luminix\/users"
            }
        },
        "login": "login",
        "logout": "logout",
        "password": {
            "request": "password\/reset",
            "email": "password\/email",
            "reset": "password\/reset\/{token}",
            "update": "password\/reset",
            "confirm": "password\/confirm"
        },
        "home": "\/",
        "profile": "profile"
        
    }
}